       <button onClick={(e => setState({...state, showingItems: !state.showingItems}))}>\n                        {state.showingItems ? \"Hide Items\" : \"Show Items\"}\n                    </button>\n                    <button onClick={cancelOrder}> Cancel Order </button>\n                </div>\n            { state.showingItems &&\n                <table style={{width:\"80%\"}}>\n                    <thead>\n                        <tr>\n                            <th>Item</th><th>price</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                    {order.menuItems.map((item, idx) => <tr key={idx}>\n                        <td>{item.name}</td>\n                        <td>$ {item.price.toFixed(2)}</td>\n                    </tr>)}\n                    </tbody>\n                </table>\n            }\n\n\n        </div>\n    )\n}\n\nexport default DisplayOrder\n",{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":20,"column":8,"nodeType":"100","endLine":20,"endColumn":10,"suggestions":"101"},{"ruleId":"102","severity":1,"message":"103","line":2,"column":7,"nodeType":"104","messageId":"105","endLine":2,"endColumn":15},{"ruleId":"98","severity":1,"message":"106","line":27,"column":8,"nodeType":"100","endLine":27,"endColumn":10,"suggestions":"107"},{"ruleId":"102","severity":1,"message":"108","line":1,"column":10,"nodeType":"104","messageId":"105","endLine":1,"endColumn":19},{"ruleId":"109","severity":1,"message":"110","line":28,"column":28,"nodeType":"111","messageId":"112","endLine":28,"endColumn":55},{"ruleId":"98","severity":1,"message":"113","line":40,"column":8,"nodeType":"100","endLine":40,"endColumn":17,"suggestions":"114"},{"ruleId":"102","severity":1,"message":"115","line":2,"column":10,"nodeType":"104","messageId":"105","endLine":2,"endColumn":21},{"ruleId":"102","severity":1,"message":"116","line":20,"column":12,"nodeType":"104","messa